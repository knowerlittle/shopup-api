{
  "name": "popin-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./node_modules/.bin/pm2 start ./bin/www  -i max --attach",
    "dev": "cross-env NODE_ENV=development node ./node_modules/.bin/pm2 start ./bin/www --watch",
    "stop": "node ./node_modules/.bin/pm2  kill",
    "monit": "node ./node_modules/.bin/pm2  monit"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.1",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "express-jwt": "^5.3.0",
    "helmet": "^3.9.0",
    "ip": "^1.1.5",
    "morgan": "~1.9.0",
    "pm2": "^2.7.2"
  }
}
